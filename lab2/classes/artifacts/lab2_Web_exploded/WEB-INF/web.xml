<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         version="2.5"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>ControllerServlet</servlet-name>
    <servlet-class>servlets.ControllerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ControllerServlet</servlet-name>
    <url-pattern>/sendData</url-pattern>
  </servlet-mapping>



  <servlet>
    <servlet-name>AreaCheckServlet</servlet-name>
    <servlet-class>servlets.AreaCheckServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AreaCheckServlet</servlet-name>
    <url-pattern>/AreaCheckServlet</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>ClearDataServlet</servlet-name>
    <servlet-class>servlets.ClearDataServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ClearDataServlet</servlet-name>
    <url-pattern>/clearData</url-pattern>
  </servlet-mapping>



  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

</web-app>
